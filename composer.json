{
    "name": "users-au/users-au-laravel-client",
    "type": "library",
    "description": "Users.au Laravel Client",
    "homepage": "https://github.com/users-au/users-au-laravel-client",
    "license": "MIT",
    "authors": [
        {
            "name": "David Shen",
            "email": "sica@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^8.0",
        "socialiteproviders/usersau": "^1.1",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/routing": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/contracts": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Usersau\\UsersauLaravelClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Usersau\\UsersauLaravelClient\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
              "Usersau\\UsersauLaravelClient\\UsersauLaravelClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
